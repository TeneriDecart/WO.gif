        "kneeling lunge stretch": "hip flexor stretch",
        "kettlebell clean": "kettlebell clean",
        "kettlebell cleans": "kettlebell clean",
        "kettlebell clean and jerk": "kettlebell clean",
        "kettlebell clean & jerk": "kettlebell clean",
        "kettlebell clean and jerks": "kettlebell clean",
        "single-arm kettlebell clean and jerk": "kettlebell clean",
        "kettlebell power clean and jerk": "kettlebell clean",
        "kettlebell olympic clean and jerk": "kettlebell clean",
        "kettlebell clean & jerks": "kettlebell clean",
    };
    return mapping[name] || name;
}

document.getElementById("generateBtn").onclick = function() {
    const input = document.getElementById("inputText").value;
    const lines = input.split("\n");
    const outputDiv = document.getElementById("output");
    outputDiv.innerHTML = "";

    for (let line of lines) {
        line = line.trim();
        if (!line) continue;

        const firstWord = line.split(/\s+/)[0].toLowerCase();
        if (stopWords.includes(firstWord)) continue;

        const block = document.createElement("div");
        block.className = "exercise-block";

        const textEl = document.createElement("div");
        textEl.textContent = line;
        block.appendChild(textEl);

        let gifUrl = null;

        const match = line.match(/[a-zA-Z][a-zA-Z\s\-()]+/);
        if (match) {
            const rawName = match[0].trim();
            const normalizedName = normalizeExercise(rawName);

            if (gifDatabase[normalizedName]) {
                gifUrl = gifDatabase[normalizedName];
            }
        }

        if (gifUrl) {
            const img = document.createElement("img");
            img.src = gifUrl;
            block.appendChild(img);
        } else {
            const err = document.createElement("div");
            err.textContent = "❌ GIF not found in local database";
            block.appendChild(err);
        }

        outputDiv.appendChild(block);
    }
};
</script>

</body>
</html>
